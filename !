    "bot_id": "$botID",
    "user_id": "$authorID",
    "guild_id": "$guildID"
}]
$sendEmbedMessage[$channelID;;Reproduciendo musica anterior.;;**$httpResult[nextSong]**;000000;;;Akiomae APIS;;;;yes;no]

$endif

$if[$checkContains[$message[1];saltar;jump]==true]
$nomention

$httpPost[http://node.akiomae.xyz:4402/API/list;{
    "token": "$getVar[TOKEN]",
    "bot_id": "$botID",
    "guild_id": "$guildID"
}]
$if[$httpResult[songs;0;name]==]
Aun no hay musicas en la lista.$else
$title[Saltar entre músicas]
$footer[Akiomae APIS]
$addTimestamp
$description[Salta a cualquier música de la lista.]

$newSelectMenu[Musics;1;1;Selecciona una música de la lista]
$addSelectMenuOption[Musics;$httpResult[songs;0;name];jmp-0;;;]$endif

