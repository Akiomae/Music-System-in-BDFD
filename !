    "token": "$getVar[TOKEN]",
    "bot_id": "$botID",
    "user_id": "$authorID",
    "guild_id": "$guildID"
}]

$var[reanundado;$sendEmbedMessage[$channelID;;Musica reanudada.;;**$httpResult[currentTrack]**;000000;;;Akiomae APIS;;;;yes;yes]]

$endif

$if[$checkContains[$message[1];stop;parar;desconectar;disconect]==true]
$nomention

$var[ID;$sendMessage[Deteniendo musica...;yes]]
$httpPost[http://node.akiomae.xyz:4402/API/stop;{
    "token": "$getVar[TOKEN]",
    "bot_id": "$botID",
    "user_id": "$authorID",
    "guild_id": "$guildID"
}]
$description[Musica detenida y bot desconectado exitosamente
-# La lista de reproducción ha sido reseteada.]
$footer[Akiomae APIS]
$addTimestamp

$deleteMessage[$channelID;$var[ID]]
$endif

$if[$checkContains[$message[1];skip;siguiente;adelante]==true]
$nomention
$httpPost[http://node.akiomae.xyz:4402/API/skip;{
    "token": "$getVar[TOKEN]",
    "bot_id": "$botID",
    "user_id": "$authorID",
    "guild_id": "$guildID"
}]

$sendEmbedMessage[$channelID;;Reproduciendo siguiente musica.;;**$httpResult[nextSong]**;000000;;;Akiomae APIS;;;;yes;no]

$endif

$if[$checkContains[$message[1];back;atras;anterior]==true]
$nomention
