$suppressErrors

$if[$checkContains[$message[1];pausar;pause]==true]
$nomention
$httpPost[http://node.akiomae.xyz:4402/API/pause;{
    "token": "$getVar[TOKEN]",
    "bot_id": "$botID",
    "user_id": "$authorID",
    "guild_id": "$guildID"
}]

$var[pausado;$sendEmbedMessage[$channelID;;Musica pausada.;;**$httpResult[currentTrack]**;000000;;;Akiomae APIS;;;;yes;yes]]

$endif

$if[$checkContains[$message[1];resume;continue;continuar]==true]
$nomention
